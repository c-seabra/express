{
  "name": "@websummit/graphql",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "main": "src/index",
  "types": "src/index",
  "files": [
    "src"
  ],
  "scripts": {
    "build": "yarn run clean && yarn run compile",
    "build:standalone": "yarn run clean && yarn run compile",
    "clean": "rimraf -rf ./dist",
    "codegen": "graphql-codegen --config codegen.yml -r dotenv/config",
    "compile": "tsc -p tsconfig.build.json --noEmit",
    "prepublishOnly": "yarn run build",
    "fix": "eslint . --config ../../.eslintrc.js  --ignore-path ../../.eslintrelativeignore --ext js,jsx,ts,tsx --cache --cache-location ~/.eslintcache/ --fix",
    "lint": "eslint . --config ../../.eslintrc.js  --ignore-path ../../.eslintrelativeignore --ext js,jsx,ts,tsx --cache --cache-location ~/.eslintcache/ ",
    "test": "yarn run build"
  },
  "dependencies": {
    "@apollo/client": "*",
    "@graphql-typed-document-node/core": "*",
    "apollo-cache-inmemory": "*",
    "graphql": "*",
    "isomorphic-unfetch": "*",
    "react": "*"
  },
  "devDependencies": {}
}
