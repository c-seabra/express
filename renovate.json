{
  "branchConcurrentLimit": 2,
  "bumpVersion": "patch",
  "dependencyDashboard": true,
  "extends": ["config:base"],
  "packageRules": [
    {
      "automerge": true,
      "matchUpdateTypes": ["minor", "patch", "pin", "digest"]
    },
    {
      "groupName": "eslint",
      "matchPackagePatterns": ["eslint"]
    },
    {
      "groupName": "react",
      "matchPackagePatterns": ["react"]
    },
    {
      "groupName": "webpack",
      "matchPackagePatterns": ["webpack"]
    }
  ],
  "rangeStrategy": "pin",
  "reviewers": ["reggaemuffin"]
}
