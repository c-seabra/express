{
  "name": "micro",
  "description": "All your frontends are belong to us",
  "private": true,
  "license": "MIT",
  "workspaces": [
    "packages/*",
    "frontends/*"
  ],
  "scripts": {
    "build": "lerna run build",
    "build:frontends": "lerna run build --ignore @websummit/*",
    "build:packages": "lerna run build --ignore @websummit-micro/*",
    "build:standalone": "lerna run build:standalone",
    "compile": "lerna run compile",
    "compile:frontends": "lerna run compile --ignore @websummit/*",
    "compile:packages": "lerna run compile --ignore @websummit-micro/*",
    "check": "yarn build && yarn build:standalone && yarn lint && yarn is-pretty",
    "clean": "lerna run clean",
    "codegen": "lerna run codegen && yarn codegen:fix && yarn codegen:prettier",
    "codegen:prettier": "yarn codegen:prettier-fragments && yarn codegen:prettier-types",
    "codegen:prettier-fragments": "prettier --config ./.prettierrc.json --write packages/graphql/src/@types/fragments.ts",
    "codegen:prettier-types": "prettier --config ./.prettierrc.json --write packages/graphql/src/@types/operations.tsx",
    "codegen:fix": "yarn codegen:fix-fragments && yarn codegen:fix-types",
    "codegen:fix-fragments": "eslint packages/graphql/src/@types/fragments.ts --fix --cache --no-ignore",
    "codegen:fix-types": "eslint packages/graphql/src/@types/operations.tsx --fix --cache --no-ignore",
    "fix": "eslint . --ext js,ts,tsx --fix --cache",
    "format": "yarn pretty",
    "is-pretty": "prettier --config ./.prettierrc.json --check .",
    "lint": "eslint . --ext js,ts,tsx",
    "pretty": "prettier --config ./.prettierrc.json --write .",
    "pub": "lerna publish",
    "ready": "yarn fix && yarn pretty",
    "ci": "yarn ready && yarn build && yarn build:standalone",
    "start": "yarn install && yarn build:packages && yarn start:single-spa --stream",
    "start:single-spa": "lerna run start --scope @websummit-micro/* --concurrency 80",
    "start:omnia-container": "lerna run start --scope @websummit-micro/omnia-container",
    "start:omnia": "concurrently -n \"s3,webpack\" -k \"yarn run start:serve\" \"yarn run start:omnia-container\"",
    "start:serve": "yarn run build:standalone && yarn serve --cors -s builds -l 9337",
    "test": "lerna run test"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn ready || true"
    }
  },
