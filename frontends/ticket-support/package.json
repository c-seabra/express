{
  "name": "@websummit-micro/ticket-support",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "analyze": "webpack --mode=production --config ./webpack.config.build.js --env analyze",
    "build": "webpack --mode=production",
    "build:standalone": "webpack --mode=production --config ./webpack.config.build.js",
    "clean": "rimraf -rf ./dist",
    "coverage": "cross-env BABEL_ENV=test jest --coverage",
    "start": "webpack serve --port 9002",
    "start:standalone": "webpack serve --env standalone",
    "test": "jest --passWithNoTests --verbose",
    "test:watch": "jest --watch --verbose"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@apollo/client": "*",
    "@apollo/react-common": "*",
    "@websummit/components": "*",
    "@websummit/glue": "*",
    "@websummit/graphql": "*",
    "formik": "*",
    "isomorphic-unfetch": "*",
    "jwt-decode": "*",
    "luxon": "*",
    "react": "*",
    "react-dom": "*",
    "react-helmet": "*",
    "react-modal": "*",
    "react-router-dom": "*",
    "styled-components": "*",
    "yup": "*"
  },
  "devDependencies": {}
}
