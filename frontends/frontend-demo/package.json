{
  "name": "@websummit-micro/frontend-demo",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "start": "webpack serve --port 9001",
    "start:standalone": "webpack serve --env standalone",
    "build": "webpack --mode=production  --config ./webpack.config.build.js",
    "build-standalone": "webpack --mode=production --config ./webpack.config.build.js",
    "analyze": "webpack --mode=production --config ./webpack.config.build.js --env analyze",
    "lint": "eslint src --ext js,ts,tsx",
    "format": "prettier --write \"./**\"",
    "test": "cross-env BABEL_ENV=test jest",
    "watch-tests": "cross-env BABEL_ENV=test jest --watch",
    "coverage": "cross-env BABEL_ENV=test jest --coverage",
    "clean": "rimraf -rf ./dist"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && concurrently yarn:test yarn:lint"
    }
  },
  "devDependencies": {},
  "dependencies": {
    "@websummit/ws-components": "*",
    "@websummit/ws-lib-example": "*",
    "react": "*",
    "react-dom": "*"
  }
}
