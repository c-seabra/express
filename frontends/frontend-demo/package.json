{
  "name": "@websummit-micro/frontend-demo",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "analyze": "webpack --mode=production --config ./webpack.config.build.js --env analyze",
    "build": "webpack --mode=production",
    "build:standalone": "webpack --mode=production --config ./webpack.config.build.js",
    "clean": "rimraf -rf ./dist",
    "coverage": "cross-env BABEL_ENV=test jest --coverage",
    "start": "webpack serve --port 9001",
    "fix": "eslint . --config ../../.eslintrc.js  --ignore-path ../../.eslintrelativeignore --ext js,jsx,ts,tsx --cache --cache-location ~/.eslintcache/ --fix",
    "lint": "eslint . --config ../../.eslintrc.js  --ignore-path ../../.eslintrelativeignore --ext js,jsx,ts,tsx --cache --cache-location ~/.eslintcache/ ",
    "start:standalone": "webpack serve --env standalone"
  },
  "dependencies": {
    "@websummit/components": "*",
    "@websummit/ws-lib-example": "*",
    "react": "*",
    "react-dom": "*",
    "single-spa-react": "*"
  },
  "devDependencies": {}
}
