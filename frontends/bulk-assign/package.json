    "jwt-decode": "^3.0.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "typescript": "^4.0.5",
    "webpack-config-single-spa-react-ts": "^1.17.3"
  },
  "resolutions": {
    "styled-components": "^5"
